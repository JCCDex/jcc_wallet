{
  "name": "jcc_wallet",
  "version": "1.0.3",
  "description": "Toolkit of wallet to manage multiple chains & support multiple keystores for each chain",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "unpkg": "dist/jcc-wallet.min.js",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "deploy": "./publish.sh",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "tsc",
    "test": "gulp",
    "test:node": "npm run build && nyc --extension=.ts --reporter=lcov --reporter=html --reporter=text mocha --timeout=3000 test/*.spec.js",
    "test:browser": "npm run build && karma start karma.conf.js",
    "tslint": "tslint --project tsconfig.json --config tslint.json",
    "compile": "./compile.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JCCDex/jcc_wallet.git"
  },
  "keywords": [
    "jccdex",
    "blockchain",
    "wallet",
    "ethereum",
    "jingtum",
    "moac",
    "call-chain",
    "bizain-chain",
    "stream-chain",
    "swtc-chain",
    "keystore"
  ],
  "author": "JCCDex",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JCCDex/jcc_wallet/issues"
  },
  "homepage": "https://github.com/JCCDex/jcc_wallet#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "browserify": "^16.3.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "cross-env": "^5.2.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-shell": "^0.7.1",
    "jsdom": "^15.1.1",
    "karma": "^4.1.0",
    "karma-browserify": "^6.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-detect-browsers": "^2.3.3",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^4.0.2",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.35.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "@types/node": "^12.0.12",
    "bn.js": "^5.0.0",
    "clone-deep": "^4.0.1",
    "eccrypto": "^1.1.1",
    "elliptic": "^6.5.0",
    "ethereumjs-util": "^6.1.0",
    "ethereumjs-wallet": "^0.6.3",
    "jcc_common": "^0.1.0",
    "keccak": "^2.0.0",
    "lockr": "^0.8.5",
    "scryptsy": "^2.1.0",
    "swtc-chains": "^1.0.13",
    "swtc-factory": "^1.2.8"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99
  },
  "sideEffects": false
}
