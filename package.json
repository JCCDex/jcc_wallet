{
  "name": "jcc_wallet",
  "version": "0.3.5",
  "description": "Toolkit of wallet to manage multiple chains & support multiple keystores for each chain",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "deploy": "./publish.sh",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build": "tsc",
    "test": "gulp",
    "test:node": "npm run build && nyc --reporter=html --reporter=text mocha --timeout=3000 test/*.spec.js",
    "test:browser": "npm run build && karma start karma.conf.js",
    "tslint": "tslint --project tsconfig.json --config tslint.json",
    "compile": "./compile.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JCCDex/jcc_wallet.git"
  },
  "keywords": [
    "jccdex",
    "blockchain",
    "wallet",
    "ethereum",
    "jingtum",
    "moac",
    "call-chain",
    "bizain-chain",
    "stream-chain",
    "swtc-chain",
    "keystore"
  ],
  "author": "JCCDex",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JCCDex/jcc_wallet/issues"
  },
  "homepage": "https://github.com/JCCDex/jcc_wallet#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "browserify": "^16.2.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-shell": "^0.7.0",
    "jsdom": "^14.0.0",
    "karma": "^4.0.1",
    "karma-browserify": "^6.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-detect-browsers": "^2.3.3",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^3.0.5",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "ts-loader": "^6.0.2",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "@types/node": "^11.11.6",
    "call-address-codec": "^0.1.0",
    "call-keypairs": "^0.1.0",
    "chain3": "^0.1.18",
    "clone-deep": "^4.0.0",
    "ethereumjs-util": "^6.1.0",
    "ethereumjs-wallet": "^0.6.3",
    "jcc_common": "^0.1.0",
    "jcc_jingtum_base_lib": "^1.1.1",
    "keccak": "^2.0.0",
    "lockr": "^0.8.4",
    "node-forge": "^0.8.2",
    "scryptsy": "^2.0.0",
    "stm-lib": "0.0.2"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99
  },
  "sideEffects": [
    "./lib/**/*.js"
  ]
